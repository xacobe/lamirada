{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^2.3",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.6",
        "drupal/ajaxin": "^2.0",
        "drupal/auto_translation": "^1.2",
        "drupal/ckeditor5_plugin_pack": "^1.4",
        "drupal/coffee": "^2.0",
        "drupal/colorbox": "^2.1",
        "drupal/configure_contextual_links": "^1.0@alpha",
        "drupal/core-composer-scaffold": "10.5.4",
        "drupal/core-project-message": "10.5.4",
        "drupal/core-recommended": "10.5.4",
        "drupal/gin": "^4.0",
        "drupal/gin_lb": "^1.0@RC",
        "drupal/gin_toolbar": "^2.0",
        "drupal/gridstack": "^3.0",
        "drupal/inline_entity_form": "^3.0@RC",
        "drupal/layout_builder_component_attributes": "^2.2",
        "drupal/layout_builder_customizer": "^1.0",
        "drupal/layout_builder_modal": "^2.0",
        "drupal/layout_builder_reorder": "^2.0",
        "drupal/lite_youtube_embed": "^1.0",
        "drupal/mason": "^2.0",
        "drupal/media_gallery": "^3.0",
        "drupal/nomarkup": "^1.0",
        "drupal/outlayer": "^2.0",
        "drupal/paragraph_blocks": "^4.1",
        "drupal/pathauto": "^1.13",
        "drupal/photoswipe": "^5.0",
        "drupal/rabbit_hole": "^1.0",
        "drupal/redirect": "^1.11",
        "drupal/simple_block": "^1.7",
        "drupal/splide": "^2.0",
        "drupal/tmgmt": "^1.17",
        "drupal/toastify": "^1.3",
        "drupal/tome": "^1.13",
        "drupal/views_bulk_edit": "^3.0",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/viewsreference": "^2.0@beta",
        "drush/drush": "^13.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "patches": {
            "drupal/tome": {
                "Undefined array key path -https://www.drupal.org/project/tome/issues/3485337-": "https://www.drupal.org/files/issues/2024-11-04/undefined_array_key_path-3485337.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "recipes/{$name}": [
                "type:drupal-recipe"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    },
    "require-dev": {
        "drupal/devel": "^5.3"
    }
}
